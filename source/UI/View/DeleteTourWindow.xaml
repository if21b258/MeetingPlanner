<Window x:Class="TourPlannerUI.View.DeleteTourWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TourPlannerUI.View"
        mc:Ignorable="d"
        Title="DeleteTour" Height="210" Width="350">
    <Grid Margin="0,0,0,4" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment">
        <Label Height="46" Margin="48,34,30,110">Are you sure that you want to delete this tour?</Label>
        <Button Background="Green" Margin="60,95,200,46" d:LayoutOverrides="VerticalAlignment">Delete</Button>
        <Button Click="CancelButton_Click" Background="Red" Margin="210,95,55,46" d:LayoutOverrides="VerticalAlignment">Cancel</Button>
    </Grid>


</Window>
